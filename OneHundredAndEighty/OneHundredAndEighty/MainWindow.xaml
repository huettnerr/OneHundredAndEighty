<Window x:Class="OneHundredAndEighty.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:Controls="clr-namespace:OneHundredAndEighty.Controls"
        xmlns:Score="clr-namespace:OneHundredAndEighty.Score"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Height="1000"
        Width="1200"
        Title="OneHundredAndEighty"
        Closing="MainWindow_Closing"
        Foreground="#FF1F1F1F">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <LinearGradientBrush.RelativeTransform>
                <TransformGroup>
                    <ScaleTransform CenterY="0.5" CenterX="0.5" />
                    <SkewTransform CenterY="0.5" CenterX="0.5" />
                    <RotateTransform CenterY="0.5" CenterX="0.5" />
                    <TranslateTransform />
                </TransformGroup>
            </LinearGradientBrush.RelativeTransform>
            <GradientStop Color="#FF191919" Offset="0" />
            <GradientStop Color="#FF939393" Offset="1" />
        </LinearGradientBrush>
    </Window.Background>
    <TabControl x:Name="Tab"
                TabStripPlacement="Bottom"
                Background="{x:Null}"
                BorderBrush="{x:Null}"
                Foreground="{x:Null}"
                FontFamily="Agency FB"
                FontWeight="Bold"
                FontSize="18"
                SelectedIndex="0">
        <TabControl.Resources>
            <Style TargetType="TabItem">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="TabItem">
                            <Border Name="Border" BorderThickness="1,0,1,1" BorderBrush="Black" Margin="-1,0" Background="#FF111111">
                                <ContentPresenter VerticalAlignment="Center" HorizontalAlignment="Center" ContentSource="Header" Margin="10,4" />
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsSelected" Value="True">
                                    <Setter Property="Foreground" Value="#FF15EC00" />
                                    <Setter TargetName="Border" Property="Background" Value="#FF032E03" />
                                    <Setter TargetName="Border" Property="Margin" Value="0,0,0,-6" />
                                </Trigger>
                                <Trigger Property="IsSelected" Value="False">
                                    <Setter Property="Foreground" Value="#FF194400" />
                                </Trigger>
                                <Trigger Property="IsEnabled" Value="False">
                                    <Setter Property="Foreground" Value="#FF3E3E3E" />
                                    <Setter Property="Visibility" Value="Hidden" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
        </TabControl.Resources>
        <TabItem x:Name="NewGameTab"
                 Header="New Match"
                 IsSelected="True"
                 Cursor="Hand">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="3*"/>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="4*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="2*"/>
                </Grid.ColumnDefinitions>
                
                <Score:WhiteboardControl Grid.Row="0" Grid.ColumnSpan="3" />
                <GridSplitter Grid.Row="1" Grid.ColumnSpan="3" Height="1" Background="Black" Opacity="0.2" VerticalAlignment="Center" HorizontalAlignment="Stretch"/>
                
                <Controls:SettingsControl Grid.Row="2" Grid.Column="0" x:Name="SettingsControl"/>
                <Controls:InfoControl Grid.Row="2" Grid.Column="0" x:Name="InfoControl"/>
                <GridSplitter Grid.Row="2" Grid.Column="1" Width="1" Background="Black" Opacity="0.2" VerticalAlignment="Stretch" HorizontalAlignment="Center"/>
                <Controls:BoardControl Grid.Row="2" Grid.Column="2" x:Name="BoardControl"/>
            </Grid>
        </TabItem>
